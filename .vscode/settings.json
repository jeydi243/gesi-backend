{
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 300,
  "bracketSameLine": false,
  "window.title": "${folderName}",
  "editor.quickSuggestions": {
    "strings": true
  },
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "workbench.preferredDarkColorTheme": "Bearded Theme Monokai Black",
  "workbench.colorTheme": "Bearded Theme Black & Diamond",
  "editor.fontFamily": "'Cascadia Code', 'Courier New', monospace",
  "scm.inputFontFamily": "k2d",
  "mdb.confirmRunAll": false,
  "editor.roundedSelection": true,
  "restoreTerminals.terminals": [
    {
      "profile": "Git Bash",
      "splitTerminals": [
        {
          "name": "server",
          "commands": ["npm run sd"],
          "shouldRunCommands": true
        }
      ]
    }
  ],
  "better-comments.multilineComments": true,
  "files.autoSave": "afterDelay"
}
